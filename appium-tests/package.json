    "@wdio/mocha-framework": "^8.20.0",
    "@wdio/spec-reporter": "^8.20.0",
    "appium": "^2.2.0",
    "appium-uiautomator2-driver": "^2.34.0",
    "appium-xcuitest-driver": "^5.10.0",
    "chai": "^4.3.10"
  }
