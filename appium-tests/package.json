{
  "name": "meditrack-appium-tests",
  "version": "1.0.0",
  "description": "Appium mobile automation tests for MediTrack application",
  "main": "index.js",
  "scripts": {
    "test": "wdio run ./wdio.conf.js",
    "test:android": "wdio run ./wdio.android.conf.js",
